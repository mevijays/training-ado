{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceGroupName": {
            "value": "rg-demo-aks"
        },
        "location": {
            "value": "eastus"
        },
        "clusterName": {
            "value": "aks-demo"
        },
        "dnsPrefix": {
            "value": "aksdemo"
