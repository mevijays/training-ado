        "minNodeCount": {
            "value": 1
        },
        "maxNodeCount": {
            "value": 5
        },
        "networkPlugin": {
            "value": "azure"
        },
        "tags": {
            "value": {
                "environment": "demo",
                "managed_by": "bicep"
            }
        }
    }
